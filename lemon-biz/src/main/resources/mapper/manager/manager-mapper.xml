<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="manager">

	<insert id="insertRank">
		insert into 
			rank 
		values(
			seq_rank_key.nextval, 
		    #{ name }
		)
	</insert>
	
</mapper>